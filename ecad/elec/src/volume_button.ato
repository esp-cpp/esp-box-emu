from "generics/buttons.ato" import ButtonPulldown
from "generics/interfaces.ato" import Power

from "TL3340AF160QG.ato" import TL3340AF160QG

module VolumeButton:
    """
    Volume button using a TL3340AF160QG
    """
    signal out
    power = new Power
    btn = new ButtonPulldown
    btn.btn -> TL3340AF160QG
    btn.sheild = power.gnd
    power ~ btn.power
    out ~ btn.out
