from "gbc-a-b.ato" import GbcAB

module GbcABXY:
    """
    A module which has A, B, X, and Y buttons
    using the A/B buttons from the GBC
    """
    signal a
    signal b
    signal x
    signal y
    a_b_btns = new GbcAB
    x_y_btns = new GbcAB
    a ~ a_b_btns.a
    b ~ a_b_btns.b
    x ~ x_y_btns.a
    y ~ x_y_btns.b
